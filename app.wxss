/**app.wxss**/
.custom{
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  height: 45px;
  background: #fff;
  z-index: 999;
  display: flex;
  align-items: center;
}
.custom text{
  color: #fff;
  font-size: 34rpx;
  font-weight: 500;
  width: 200rpx;
  max-width: 280rpx;
  position: absolute;
  left: 50%;
  margin-left: -100rpx;
  text-align: center;
}

