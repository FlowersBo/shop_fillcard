/* pages/user/twoDimension/index.wxss */
page{
  width: 100%;
  height: 100%;
  background-color: #F2F2F2;
}
.twoDimensionWrap{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.twoDimension{
  width: 94%;
  height: 900rpx;
  background-color: #fff;
  border-radius: 60rpx;
  margin-top: 100rpx;
  box-sizing: border-box;
  padding: 50rpx 30rpx;
  font-size: 36rpx;
}
.shopLocation{
  color: #ddd;
  font-size: 28rpx;
  margin-top: 15rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
}
.shopImg{
  width: 100%;
  height: 600rpx;
  margin-top: 70rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.shopImg image{
  width: 600rpx;
  height: 600rpx;
}