/* pages/user/index.wxss */
page{
  width: 100%;
  height: 100%;
  background-color: #F2F2F2;
}

.container .list{
  width:100%;
  margin-top:20rpx;
  background-color: #fff;
}
.container .list .item{
  height: 100rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  padding: 25rpx 0;
  font-size: 32rpx;
  border-bottom: 1rpx solid #ccc;
  padding-left: 16rpx;
  color: rgb(19, 18, 18);
}
.titlename{
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
}
.lastChild_item{
  box-sizing: border-box;
  display: flex;
  align-items: center;
  padding: 25rpx 0;
  font-size: 32rpx;
  border-bottom: 1rpx solid #ccc;
  padding-left: 16rpx;
  color: rgb(19, 18, 18);
}
.lastChild{
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 10;
  -webkit-box-orient: vertical;
  line-height: 1.6;
}